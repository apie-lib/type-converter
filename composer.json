{
  "name": "apie/type-converter",
  "description": "Converts objects from specific type to an other type.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\TypeConverter\\": "src/"
    }
  },
  "autoload-dev": {
